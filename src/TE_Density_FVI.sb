#!/bin/bash -login
#SBATCH -J FVI_TE_Density
#SBATCH --time=7:00:00
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=7
#SBATCH --mem-per-cpu=30G
#SBATCH -o /mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/results/FVI_Density.out
#--------------------------------------------------------
TE_DIR=/mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/TE_Density
DATA_DIR=/mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/results
GENOME="FVI"


# Load Python
module purge
module load GCCcore/11.3.0 Python/3.10.4

# Source the Python packages that are version controlled
source /mnt/research/edgerpat_lab/Scotty/venvs/S_Domestication/bin/activate

# Go to project directory
cd /mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/

# Run the code
python $TE_DIR/process_genome.py $DATA_DIR/Cleaned_FVI_maker_annotation.tsv $DATA_DIR/Cleaned_FVI_NewNames.fasta.mod.EDTA.TEanno.tsv $GENOME -c $TE_DIR/config/production_run_config.ini  -n 7 -o $DATA_DIR

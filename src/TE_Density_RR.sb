#!/bin/bash -login
#SBATCH -J RR_TE_Density
#SBATCH --time=12:00:00
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=10
#SBATCH --mem-per-cpu=40G
#SBATCH -o /mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/results/RR_Density.out
#--------------------------------------------------------
TE_DIR=/mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/TE_Density
DATA_DIR=/mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/results
GENOME="RR"


# Load Python
module purge
module load GCCcore/11.3.0 Python/3.10.4

# Source the Python packages that are version controlled
source /mnt/research/edgerpat_lab/Scotty/venvs/S_Domestication/bin/activate

# Go to project directory
cd /mnt/research/edgerpat_lab/Scotty/Strawberry_Domestication/

# Run the code
python $TE_DIR/process_genome.py $DATA_DIR/Cleaned_Royal_Royce_Gene_Annotation.tsv $DATA_DIR/Cleaned_Royal_Royce.fasta.mod.EDTA.TEanno.tsv $GENOME -c $TE_DIR/config/production_run_config.ini  -n 10 -o $DATA_DIR


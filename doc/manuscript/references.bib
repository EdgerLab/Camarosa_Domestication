file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Edger et al. - 2019 - Origin and evolution of the octoploid strawberry genome.pdf:pdf},
issn = {15461718},
journal = {Nature Genetics},
number = {March},
pages = {541--547},
publisher = {Springer US},
title = {{Origin and evolution of the octoploid strawberry genome}},
url = {http://dx.doi.org/10.1038/s41588-019-0356-4},
volume = {51},
